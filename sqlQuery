CREATE TABLE users (user_id INTEGER PRIMARY KEY AUTOINCREMENT,name TEXT NOT NULL,email TEXT UNIQUE NOT NULL,password TEXT NOT NULL,is_active INTEGER DEFAULT 1,created_on DATETIME DEFAULT CURRENT_TIMESTAMP,is_email_verified INTEGER DEFAULT 0);




